<section>
        <div id="tracks-title">
                <h2>Title</h2>
                <h2>Listen</h2>
        </div>
   
        {{#each tracks}}
                <ul>
                        <li id="tracks-info">
                          {{this.name}}
                           <audio controls>
                             <source src="{{this.preview_url}}" type="audio/mp3">
                           </audio>
                        </li>
                </ul>
         {{/each}}
                
</section>  

<button id="home-button"> <a href="/"> Search Another Artist</a></button>
